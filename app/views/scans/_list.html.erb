<table>
  <caption>Scans (<%=h scans.size %>)</caption>
    
  <thead>
    <tr>
      <th>Name</th>
      <th>Hosts</th>
    </tr>
  </thead>
  
  <tbody>
<% for scan in scans %>
    <tr>
      <td><%=h scan.name %></td>
      <td><%=h scan.hosts_count %></td>
      <td><%= link_to 'Show', scan %></td>
    </tr>
<% end %>
  </tbody>
</table>